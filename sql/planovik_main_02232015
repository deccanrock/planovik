CREATE DEFINER=`root`@`localhost` PROCEDURE `sp_delete_user`(
	IN inusername VARCHAR(100)
)
BEGIN

	update users set enabled=0, date_updated=UTC_TIMESTAMP() where username=inusername;

END ;;
DELIMITER ;
