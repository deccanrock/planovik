CREATE DEFINER=`root`@`localhost` PROCEDURE `sp_manageuser`(
        IN  fullname		            VARCHAR(60), 
        IN  password             		VARCHAR(60), 
        IN  inusername                	VARCHAR(100),
        IN  phone			         	VARCHAR(20), 
        IN 	designation					VARCHAR(20),
        IN  reportstoemail				VARCHAR(100),
        IN	createdbyemail				VARCHAR(100),
        IN 	level						INT(3),
        IN  tzoffset					INT(6),
        IN  mode						VARCHAR(10),
        OUT regerror					VARCHAR(160)
)
BEGIN

		DECLARE sqltext VARCHAR(300);
        DECLARE createdby INT(10);
        DECLARE reportsto INT(10);
        
		Select id INTO @createdby from users where username=createdbyemail;
		Select id INTO @reportsto from users where username=reportstoemail;        
        		       
        IF (SELECT STRCMP(mode, 'Create') = 0) THEN
			BEGIN

				SET @sqltext := concat(
							 'INSERT INTO users', ' (
							   fullname, 
							   username, 
							   password, 
							   phone, 
							   designation, 
							   reportsto,
							   createdby,
							   level,
                               tzoffset,
                               date_created,
                               date_updated
							) VALUES (',
							   '\'', fullname, '\'',',',
							   '\'', inusername, '\'',',',
							   '\'', password, '\'',',', 
							   '\'', phone, '\'',',', 
							   '\'', designation, '\'',',',
							   @reportsto,',',
							   @createdby,',',
							   level,',',
							   tzoffset,',',
							   '\'', UTC_TIMESTAMP(), '\'',',',
							   '\'', UTC_TIMESTAMP(), '\'',
							 ')');

				PREPARE stmt FROM @sqltext;
				EXECUTE stmt;
				DEALLOCATE PREPARE stmt;
            END;
        ELSEIF (SELECT STRCMP(mode, 'Save') = 0) THEN

			UPDATE users SET fullname=fullname, phone=phone, designation=designation, reportsto=@reportsto, level=level,
							 tzoffset=tzoffset, date_updated=UTC_TIMESTAMP where username=inusername;

        ELSEIF (SELECT STRCMP(mode, 'Disable') = 0) THEN
			
            UPDATE users SET enabled=0 where username=inusername;
        
        END IF;

END ;;
DELIMITER ;
/*!50003 SET sql_mode              = @saved_sql_mode */ ;
/*!50003 SET character_set_client  = @saved_cs_client */ ;
/*!50003 SET character_set_results = @saved_cs_results */ ;
/*!50003 SET collation_connection  = @saved_col_connection */ ;
