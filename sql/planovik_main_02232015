CREATE DEFINER=`root`@`localhost` PROCEDURE `sp_getitinactivitymasterdet`(
	IN  initinnum INT(11),
    IN  inversion INT(6)
)
BEGIN
	Select status, isactivityhotel, isactivityother, isactivitysightseeing, isactivitytravel, datecreated, dateupdated from 
     itin_activity_master where itinnum = initinnum and version = inversion;
END ;;
