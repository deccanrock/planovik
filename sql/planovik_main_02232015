CREATE DEFINER=`root`@`localhost` PROCEDURE `sp_getpasssalt`(
        IN  username	               	VARCHAR(20),
        OUT salt						VARCHAR(16),
        OUT pass						VARCHAR(20)
)
BEGIN
		select salt, pass INTO salt, pass from users where username = username;
END ;;
DELIMITER ;
