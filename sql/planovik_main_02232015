CREATE DEFINER=`root`@`localhost` PROCEDURE `sp_user_exists`(
        IN  inusername		               	VARCHAR(120),
        OUT userid							INT(10)
)
BEGIN
		select id into userid from users where username = inusername;
        IF (userid IS NULL) THEN
			SET userid = 0;
		END IF;
END ;;
