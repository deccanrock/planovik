CREATE DEFINER=`root`@`localhost` PROCEDURE `sp_getuserdetails`(
        IN  	inusername VARCHAR(100)
)
BEGIN
    
	Select id, fullname, username, phone, designation, level, enabled, accountNonExpired,
    accountNonLocked, credentialsNonExpired, tzoffset, date_created, date_updated from users where username = inusername;

END ;;
